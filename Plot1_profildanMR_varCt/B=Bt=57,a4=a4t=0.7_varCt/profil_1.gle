size 24 20                              !Sets the size of the paper
 set texlabels 1
amove 0.5 10.5
begin graph
        size 12 10                      !Sets the size of the graph
        data "TOV/profil-TOVaniso.dat"           d10=c1,c2 
		data "nonTOV/profil_CT=.100D+06_aniso.dat"   d20=c1,c2 
        data "nonTOV/profil_CT=.100D+07_aniso.dat"   d30=c1,c2 
		data "nonTOV/profil_CT=.100D+08_aniso.dat"   d40=c1,c2 
		yaxis min 0 max 300 dticks 100 hei .5
		xaxis min 0 max 12 dticks 4 hei .5
        ytitle "$P ($ MeV fm$^{-3})$"  hei .5
		xtitle "$r ($km$)$" hei .5
		d10 line lstyle 1 lwidth 0.06 color orange
		d20 line lstyle 2 lwidth 0.06 color blue
        d30 line lstyle 3 lwidth 0.06 color red
        d40 line lstyle 4 lwidth 0.06 color green
		!key pos tl hei 0.4
end graph 
