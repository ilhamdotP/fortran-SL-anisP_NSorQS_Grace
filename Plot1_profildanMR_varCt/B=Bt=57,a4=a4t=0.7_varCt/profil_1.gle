		data "nonTOV/profil_CT=.100D+06_aniso.dat"   d20=c1,c3 
        data "nonTOV/profil_CT=.100D+07_aniso.dat"   d30=c1,c3 
		data "nonTOV/profil_CT=.100D+08_aniso.dat"   d40=c1,c3 
		yaxis min 0 max 2.5 dticks .5 hei .5
		xaxis min 0 max 12 dticks 4 hei .5
        ytitle "$m (M_{\odot})$"  hei .5
		xtitle "$r ($km$)$" hei .5
		d10 line lstyle 1 lwidth 0.06 color orange
		d20 line lstyle 2 lwidth 0.06 color blue
        d30 line lstyle 3 lwidth 0.06 color red
        d40 line lstyle 4 lwidth 0.06 color green
		!key pos tl hei 0.4
end graph 



begin key
position tl
 hei .4
nobox
line lstyle 1 lwidth 0.06 color orange text "TOV"
line lstyle 2 lwidth 0.06 color blue  text "$\tilde{c}=10^{5} $m$^2$"
line lstyle 3 lwidth 0.06 color red   text "$\tilde{c}=10^{6} $m$^2$"
line lstyle 4 lwidth 0.06 color green text "$\tilde{c}=10^{7} $m$^2$"
end key

begin key
position br
 hei .4
nobox
text "$\sigma=\sigma_{Grace}$"
! text "$C^~=Dl_p^2$"
end key



