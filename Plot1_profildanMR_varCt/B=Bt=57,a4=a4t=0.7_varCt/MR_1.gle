		data "nonTOV/radmass_CT=.100D+08_aniso.dat"  d40=c2,c4 
        !data "TOV/radmass-TOViso_MIT.dat"          d50=c2,c4 
		yaxis min 0 max 2.5 dticks 0.5 hei .5
		xaxis min 0 max 3.5 dticks 1 hei .5
        ytitle "$M (M_{\odot})$"  hei .5
		xtitle "$\rho_c (10^3 $ MeV fm$^{-3})$" hei .5
