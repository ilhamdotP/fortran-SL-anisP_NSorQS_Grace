size 24 20                              !Sets the size of the paper
 set texlabels 1
amove 0.5 0.5
begin graph
        size 12 10                      !Sets the size of the graph
        data "profil-TOVaniso.dat"           d10=c2,c4 
        data "profil_CT=.000D+00_aniso.dat"           d20=c2,c6 
        data "profil-TOVaniso_MIT.dat"           d30=c2,c4 
        data "profil_CT=.000D+00_anisoMIT.dat"           d40=c2,c6 
		!yaxis min 0 max 300 dticks 100 hei .5
		xaxis min 0 max 300 dticks 100 hei .5
        ytitle "$\rho (10^3$ MeV fm$^{-3})$"  hei .5
		xtitle "$P ($ MeV fm$^{-3})$" hei .5
		d10 line lstyle 1 lwidth 0.06 color orange
		d20 line lstyle 2 lwidth 0.06 color green
		d30 line lstyle 3 lwidth 0.06 color brown
        d40 line lstyle 4 lwidth 0.06 color pink
		!key pos tl hei 0.4
end graph 
